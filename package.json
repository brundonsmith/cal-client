  "license": "ISC",
  "dependencies": {
    "marked": "^0.3.6",
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "install": "^0.8.7",
    "webpack": "^2.3.2"
  }
}
